{
  "semi": false,
  "tabWidth": 2,
  "printWidth": 80,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "useTabs": false,
  "quoteProps": "as-needed",
  "proseWrap": "preserve",

  "plugins": ["prettier-plugin-tailwindcss"]
}
